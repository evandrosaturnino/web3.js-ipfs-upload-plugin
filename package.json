    "@chainsafe/eslint-config": "^2.0.0",
    "@cypress/webpack-preprocessor": "5.15.5",
    "@types/chai": "^4.3.10",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.6",
    "@types/ws": "8.5.4",
    "chai": "^4.3.10",
    "cypress": "^13.5.1",
    "cypress-jest-adapter": "0.1.1",
    "eslint": "8",
    "jest": "^29.5.0",
    "jest-extended": "^4.0.0",
    "npm-auth-to-token": "1.0.0",
    "process": "0.11.10",
    "ts-jest": "^29.1.0",
    "ts-loader": "9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
